
# CEF Handler
if exists(.message) {
    .event.original = del(.message)
    parsed, err = parse_cef(.event.original)
    if err == null {
        .cef = object!(parsed)
    }
}
.event.kind = "event"
.event.category = ["security"]
.log.format = "cef"
.observer.type = "security"
. = compact(., string: true, array: true, object: true, null: true)
