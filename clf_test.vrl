
# CLF Handler
if exists(.message) {
    .event.original = del(.message)
    parsed, err = parse_common_log(.event.original)
    if err == null {
        .clf = object!(parsed)
    }
}
.event.kind = "event"
.event.category = ["web"]
.log.format = "clf"
.observer.type = "web-server"
. = compact(., string: true, array: true, object: true, null: true)
