
# Syslog Handler
if exists(.message) {
    .event.original = del(.message)
    parsed, err = parse_syslog(.event.original)
    if err == null {
        .log.syslog = object!(parsed)
    }
}
.event.kind = "event"
.event.category = ["system"]
.log.format = "syslog"
. = compact(., string: true, array: true, object: true, null: true)
